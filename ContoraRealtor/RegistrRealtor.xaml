        xmlns:local="clr-namespace:ContoraRealtor"
        mc:Ignorable="d"
        Title="RegistrRealtor" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="RealtorLastName" HorizontalAlignment="Left" Height="23" Margin="289,27,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
